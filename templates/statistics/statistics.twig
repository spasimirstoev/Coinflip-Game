{% include './common/header.twig' %}
<div class="row">
	<h3 class="col-md-offset-4 headingTitle">Check our statistics and make your bet now!</h3>
</div>
<div class="row">
	<table class="table table-hover">
    <thead>
      <tr>
        <th>Head chosen</th>
        <th>Tail chosen</th>
        <th>Head win</th>
        <th>Tail win</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td>{{headStatistics}}&#37;</td>
        <td>{{tailStatistics}}&#37;</td>
        <td>{{headWin}}&#37;</td>
        <td>{{tailWin}}&#37;</td>
      </tr>
    </tbody>
  </table>
</div>